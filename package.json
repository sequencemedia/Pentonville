{
  "name": "pentonville",
  "version": "1.0.9",
  "description": "Pentonville",
  "main": "./index.js",
  "keywords": [
    "Pentonville",
    "React",
    "tab",
    "tabindex",
    "input",
    "HTML",
    "element",
    "focus"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/sequencemedia/pentonville.git"
  },
  "author": "Jonathan Perry for Sequence Media Limited",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -d lib -w",
    "prepublishOnly": "npm run build",
    "lint": "eslint *.js src stories",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "npm run lint && npm run build && git add lib"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "core-js": "^3.2.1",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
